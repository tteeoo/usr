#screenshots
alt + s
	maim -s ~/media/images/scrot/$(date +%s).png
alt + f
	maim ~/media/images/scrot/$(date +%s).png

# firefox
super + c
    firefox

# slock
super + z
    slock 

# terminal
super + Return
    st

# python shell
super + p 
    st -e python

#reload sxhkd
super + Escape
    pkill -USR1 -x sxhkd
