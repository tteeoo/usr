# screenshots
alt + s
    maim -s ~/media/images/scrot/(date +%s).png
alt + f
    maim ~/media/images/scrot/(date +%s).png

# volume
alt + j
   amixer -q sset Master 5%- && pkill -RTMIN+10  dwmblocks
alt + k
   amixer -q sset Master 5%+ && pkill -RTMIN+10 dwmblocks

# python shell
alt + p 
    st -e python

# ranger fm
alt + r
    st -e ranger

# wallpaper
alt + b
    randwall

# reload sxhkd
alt + Escape
    pkill -USR1 -x sxhkd

# firefox
alt + c
    firefox

# bloated spyware
alt + d
    discord

# slock
super + z
    lock 

# terminal
super + Return
    st
