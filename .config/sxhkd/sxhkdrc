#screenshots
alt + s
    maim -s ~/media/images/scrot/(date +%s).png
alt + f
    maim ~/media/images/scrot/(date +%s).png

# volume
alt + j
   amixer -q sset Master 5%- && pkill -RTMIN+10  dwmblocks
alt + k
   amixer -q sset Master 5%+ && pkill -RTMIN+10 dwmblocks

# firefox
super + c
    firefox

# slock
super + z
    slock 

# terminal
super + Return
    st

# python shell
super + p 
    st -e python

#reload sxhkd
super + Escape
    pkill -USR1 -x sxhkd
